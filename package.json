{
  "name": "rxstate",
  "version": "0.0.0-development",
  "description": "Simple rxjs based state management system",
  "author": "Allan Doll√© <hello@allandolle.fr> (https://allandolle.fr/)",
  "homepage": "https://github.com/blephy/rxstate#readme",
  "bugs": "https://github.com/blephy/rxstate/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blephy/rxstate.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.js",
  "files": [
    "/lib/**/*"
  ],
  "types": "./lib/index.d.ts",
  "scripts": {
    "clean": "rm -rfv reports coverage build dist lib .eslintcache",
    "commit": "cz",
    "format:ci": "prettier --check .",
    "format:fix": "prettier --write .",
    "format:markdown": "markdownlint-cli2-fix '**/*.md'",
    "format:packagejson": "prettier-package-json --write ./package.json",
    "lint:ci": "eslint . --ext js,ts --format json -o reports/eslint/rxstate-eslint-junit.json",
    "lint:fix": "eslint . --ext js,ts --fix",
    "lint:markdown": "markdownlint-cli2 '**/*.md'",
    "lint:packagejson": "npmPkgJsonLint .",
    "semantic-release": "semantic-release",
    "test": "jest",
    "test:ci": "jest --silent --coverage --runInBand --ci --bail false",
    "test:stryker": "stryker run",
    "ts:build": "tsc -b tsconfig.build.json",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "rxjs": "^7.5.5",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^5.6.1",
    "@stryker-mutator/jest-runner": "^5.6.1",
    "@stryker-mutator/typescript-checker": "^5.6.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsdoc": "^39.0.0",
    "eslint-plugin-rxjs": "^5.0.2",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "markdownlint-cli2": "^0.4.0",
    "npm-package-json-lint": "^6.3.0",
    "prettier": "^2.6.1",
    "prettier-package-json": "^2.6.3",
    "semantic-release": "^19.0.2",
    "semantic-release-npm-github-publish": "^1.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "packageManager": "yarn@3.2.0",
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "management",
    "rxjs",
    "state",
    "state-management",
    "store",
    "typescript"
  ]
}
